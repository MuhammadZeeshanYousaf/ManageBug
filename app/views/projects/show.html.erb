    <%# TABLE %>
    <table class="w-100">
      <tr style="background-color: #F9FAFC; height: 40px;">
        <th class="th-bugs">
          <form class="form-check checkbox-lg d-flex align-items-center justify-content-center">
            <input class="form-check-input rounded-0 border border-2 border-dark" type="checkbox" value="" id="flexCheckChecked" style="border-color: #3A3541AD;">
          </form>
        </th>
        <th class="th-bugs">
          Bug Details
        </th>
        <th class="th-bugs text-center">Status</th>
        <th class="th-bugs text-center">Due Date</th>
        <th class="th-bugs text-center">Assigned To</th>
        <th class="th-bugs text-center">Action</th>
      </tr>
      <% @project.bugs.each do |bug| %>
        <tr style="height: 60px;" class="border">
          <td>
            <form class="form-check checkbox-lg d-flex align-items-center justify-content-center">
              <input class="form-check-input rounded-0 border border-2 border-dark" type="checkbox" value="" id="flexCheckChecked" style="border-color: #3A3541AD;">
            </form>
          </td>
          <td data-bs-toggle="modal" data-bs-target="#bug_detail_modal_<%= bug.id %>" style="cursor: pointer;">
            <div class="d-flex align-items-center">
              <%= content_tag(:div, "", class: "rounded-circle d-block me-2 #{bug.status == "pending" ? "empty-circle-danger" : bug.status == "in_progress" ? "empty-circle-info" : "empty-circle-success"}", style: "height: 10px; width: 10px;")%>
              <%= bug.description %>
            </div>
          </td>
          <td class="text-center">
            <%= link_to bug.status == "in_progress" ? "In progress" : bug.status.capitalize, "#", 
              class: "text-decoration-none my-2 p-2 rounded #{bug.status == "pending" ? "custom-bg-danger" : bug.status == "in_progress" ? "custom-bg-info" : "custom-bg-success"}" 
            %>
          </td>
          <td data-bs-toggle="tooltip" data-bs-placement="top"  data-bs-title="Tooltip" class="text-center">
