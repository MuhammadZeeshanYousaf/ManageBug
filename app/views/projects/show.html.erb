                  </span>
                  <img src="/images/settings-icon.png" alt="Settings icon" class="me-3" />
                </div>
              </li>
              <li>
                <%= link_to bug_path(bug, status: "pending", project_id: @project.id), class: "dropdown-item", data: { turbo_method: :put } do %>
                  <span class="custom-bg-danger p-2">Pending</span>
                <% end %>
              </li>
              <li class="mt-2">
                <%= link_to bug_path(bug, status: "in_progress", project_id: @project.id), class: "dropdown-item", data: { turbo_method: :put } do %>
                  <span class="custom-bg-info p-2">In progress</span>
                <% end %>
              </li>
              <% if bug.bug_type == "bug" %>
                <li class="mt-2">
                  <%= link_to bug_path(bug, status: "resolved", project_id: @project.id), class: "dropdown-item", data: { turbo_method: :put } do %>
                    <span class="custom-bg-success p-2">Resolved</span>
                  <% end %>
                </li>
              <% else %>
                <li class="mt-2">
                  <%= link_to bug_path(bug, status: "closed", project_id: @project.id), class: "dropdown-item", data: { turbo_method: :put } do %>
                    <span class="custom-bg-success p-2">Close</span>
                  <% end %>
                </li>
              <% end %>
              <li><hr class="dropdown-divider" /></li>
              <li class="mt-2">
                <%= link_to bug_path(bug, project_id: @project.id), class: "dropdown-item", data: { turbo_method: :delete } do %>
